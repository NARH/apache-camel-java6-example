<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
   http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd
   http://camel.apache.org/schema/spring
   http://camel.apache.org/schema/spring/camel-spring.xsd">

  <bean id="JAXBProcessor" class="com.github.narh.test.JAXBProcessor">
    <constructor-arg value="com.github.narh.test.RootBean"/>
  </bean>

  <camelContext xmlns="http://camel.apache.org/schema/spring">
    <route>
      <from uri="file:src/test/resources/data/in?fileName=sample.xml&amp;noop=true"/>
      <process ref="JAXBProcessor"/>
      <log message="LOG ---> ${body}" loggingLevel="INFO"/>
      <to uri="mock:result"/>
    </route>
  </camelContext>
</beans>